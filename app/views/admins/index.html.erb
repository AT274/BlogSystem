<div class="heading">
    <h3>
        ADMIN PAGE
    </h3>
</div>
<div class="admin-content">
    <table border="1" class="mx-auto">
        <tr class="legend">
            <th>id</th>
            <th>username</th>
            <th>email</th>
            <th>created_at</th>
        </tr>
        <% @users.each do |user| %>
        <tr>
            <td class="id"><%= user.id %></td>
            <td><%= user.username %> </td>
            <td><%= user.email %></td>
            <td><%= user.created_at %></td>
        </tr>
        <% end %>
    </table>
</div>